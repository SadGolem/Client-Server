@page
@model ho.Pages.Home.DefectsInfoModel
@{
}
<br>
<h2> DefectInfo </h2>
<a class='btn btn-primary btn-sm' href='CreateDefectsInfo'>New Defect Info</a>
<table class="table">

    <thead>
        <tr>
            <th>ID</th>
            <th>Type</th>
            <th>DefectLong</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model.listDefectsInfo)
        {
            <tr>
                <td>@item.ID</td>
                <td>@item.Type</td>
                <td>@item.DefectLong</td>
                <td>
                    <a class="btn btn-primary btn-sm" href="EditDefectInfo?id=@item.ID">Edit</a>
                    <a class="btn btn-primary btn-sm" href="DeleteDefectInfo?id=@item.ID">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>